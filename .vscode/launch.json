    "version": "0.2.0",
    "configurations": [
        {
            "name": "Parse EN-US",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/nlp.exe",
            "args": ["-ANA","${workspaceFolder}/analyzers/parse-en-us","-WORK","${workspaceFolder}","${workspaceFolder}/analyzers/parse-en-us/input/doj.txt","-DEV"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
